{
  "extends": [
    "p5js",
    "p5js/dom",
    "p5js/sound"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "engines": {
    "node": "8.x"
  },
  "rules": {
    "linebreak-style": [2, "unix"],
    "default-case": 2,
    "indent": [1, 2],
    "no-unexpected-multiline": 2,
    "semi": 0,
    "consistent-return": 2,
    "no-fallthrough": 2,
    "default-case": 2,
    "no-console": 0,
    "quotes": 0,
    "guard-for-in": 2,
    "strict": [2, "global"],
    "no-unused-vars": 0,
    "eqeqeq": 2,
    "no-native-reassign": 2
  }
}
