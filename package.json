{
  "devDependencies": {
    "babel-eslint": "^6.1.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "eslint": "^3.0.1",
    "eslint-plugin-react": "^5.2.2",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "start": "node app",
    "dev": "concurrently -k \"npm run watch\" \"nodemon app\"",
    "postinstall": "browserify client/index.js -o public/bundle.js -t [ babelify --presets [ es2015 react ] ]",
    "watch": "watchify client/index.js -o public/bundle.js -t [ babelify --presets [ es2015 react ] ]"
  },
  "dependencies": {
    "concurrently": "^2.2.0",
    "express": "^4.14.0",
    "react": "^15.2.0",
    "react-dom": "^15.2.0"
  }
}
